  "dependencies": {
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "eslint": "^9.7.0",
    "eslint-plugin-markdown": "^5.1.0",
    "globals": "^15.8.0",
    "typescript-eslint": "^8.35.1"
  }
}
